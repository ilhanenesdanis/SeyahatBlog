@using SeyahatSite.Models.Sınıflar
@model List<Hakkımızda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Test.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var x in Model)
            {


                <img src="@x.ResimUrl" alt="" />
                <p>@x.Hakkımda</p>
            }

        </div>
    </div>
</div>

